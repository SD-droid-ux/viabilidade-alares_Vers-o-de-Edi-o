{
  "name": "cto-route-finder-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/check-backend.js",
    "dev": "concurrently -n \"FRONTEND,BACKEND\" -c \"cyan,magenta\" \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite dev",
    "dev:backend": "node scripts/start-backend.js",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npx serve -s dist -l ${PORT:-3000}",
    "postinstall": "echo 'Dependencies installed successfully'",
    "build:prod": "NODE_ENV=production vite build"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "svelte": "^4.2.7",
    "vite": "^5.0.0",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.2",
    "xlsx": "^0.18.5",
    "html2canvas": "^1.4.1",
    "serve": "^14.2.0",
    "ag-grid-community": "^32.0.0",
    "ag-grid-svelte": "^32.0.0"
  }
}

